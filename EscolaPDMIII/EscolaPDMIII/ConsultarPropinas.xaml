<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EscolaPDMIII.ConsultarPropinas">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Consultar Propinas">

            </Label>
            <ListView x:Name="listaPropinas">
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <TextCell 
                            Detail="{Binding Pago}"
                            Text="{Binding Mes}">

                        </TextCell>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>